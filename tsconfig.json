{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "lib": ["esnext"],
    "jsx": "react-native",
    "allowJs": false,
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["react-native", "jest"]
  },
  "include": ["App.tsx", "src"]
}